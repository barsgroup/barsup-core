{
  "controller": {
    "__default__": {
      "__realization__": "barsup.controller.DictController"
    },
    "User": {
      "service": "UserService"
    },
    "Role": {
      "service": "RoleService"
    },
    "UserRole": {
      "service": "UserRoleService"
    },
    "PermissionController": {
      "__realization__": "barsup.auth.controller.PermissionController"
    },
    "PermissionAction": {
      "__realization__": "barsup.auth.controller.PermissionAction"
    },
    "RolePermission": {
      "service": "RolePermissionService"
    },
    "Authentication": {
      "service": "AuthenticationService",
      "__realization__": "barsup.auth.controller.Authentication"
    }
  },
  "session": {
    "default": {
      "__type__": "singleton",
      "__realization__": "barsup.session.InMemory"
    }
  },
  "service": {
    "__default__": {
      "__realization__": "barsup.service.Service",
      "adapters": []
    },
    "RolePermissionService": {
      "model": "permission"
    },
    "AuthenticationService": {
      "__realization__": "barsup.auth.service.AuthenticationService",
      "model": "web_session",
      "user_model:model": "user"
    },
    "AuthorizationService": {
      "__realization__": "barsup.auth.service.AuthorizationService",
      "model": "user_role_permission",
      "user_role:model": "user_role"
    },
    "UserService": {
      "model": "user"
    },
    "RoleService": {
      "model": "role"
    },
    "UserRoleService": {
      "model": "user_role"
    }
  },
  "model": {
    "__default__": {
      "__realization__": "barsup.schema.Model",
      "db_mapper": "default",
      "session": "default"
    },
    "user": {
      "$name": "user"
    },
    "web_session": {
      "$name": "web_session"
    },
    "role": {
      "$name": "role"
    },
    "permission": {
      "$name": "permission"
    },
    "user_role": {
      "$name": "user_role",
      "$joins": [
        [
          "user_id",
          "==",
          "id",
          [
            "user",
            []
          ]
        ],
        [
          "role_id",
          "==",
          "id",
          [
            "role",
            []
          ]
        ]
      ],
      "$select": [
        "id"
      ]
    },
    "user_role_permission": {
      "$name": "user_role",
      "$joins": [
        [
          "user_id",
          "==",
          "id",
          [
            "user",
            []
          ]
        ],
        [
          "role_id",
          "==",
          "id",
          [
            "role",
            []
          ]
        ],
        [
          "role_id",
          "==",
          "role_id",
          [
            "permission",
            []
          ]
        ]
      ]
    }
  },
  "db_mapper": {
    "default": {
      "__realization__": "barsup.tests.auth.DBMapperAuth",
      "__type__": "singleton",
      "session": "default",
      "$path": [
        "auth",
        "schema.json"
      ]
    }
  },
  "api": {
    "default": {
      "middleware": [
        "log_errors_to_stderr",
        "transact",
        "authentication",
        "authorization"
      ]
    }
  },
  "frontend": {
    "default": {
      "initware": ["list_actions"],
      "$spec": {
      }
    }
  },
  "initware": {
    "list_actions": {
      "__type__": "static",
      "__realization__": "barsup.auth.list_actions"
    }
  },
  "middleware": {
    "__default__": {"__type__": "static"},
    "log_errors_to_stderr": {
      "__realization__": "barsup.middleware.log_errors_to_stderr"
    },
    "authorization": {
      "__type__": "singleton",
      "__realization__": "barsup.auth.middleware.authorization",
      "auth:service": "AuthorizationService",
      "$white_list": [
        "Authentication"
      ],
      "$preserve_user": [
        "MsgUsers",
        "Message"
      ]
    },
    "authentication": {
      "__type__": "singleton",
      "__realization__": "barsup.auth.middleware.authentication",
      "auth:service": "AuthenticationService",

      "$white_list": [
        "Authentication"
      ]
    },
    "transact": {
      "__type__": "singleton",
      "__realization__": "barsup.middleware.transact",
      "session": "default"
    }
  }
}
